{
  "name": "swish_backend",
  "version": "1.0.0",
  "description": "Backend application for Swish",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "build:watch": "tsc -w",
    "prestart": "npm run build",
    "start": "cross-env node --require dotenv/config dist/main.js",
    "start:watch": "cross-env node-dev --require dotenv/config --no-deps --respawn dist/main.js",
    "doc": "typedoc --out docs",
    "test": "cross-env NODE_ENV=test jest --setupFiles dotenv/config --verbose",
    "test:coverage": "cross-env NODE_ENV=test jest --setupFiles dotenv/config --verbose --collectCoverage",
    "dev": "concurrently \"npm:build:watch\" \"npm:start:proxy\" \"npm:start:redis\"",
    "dev:old": "cross-env NODE_ENV=dev ts-node-dev --require dotenv/config src/main.ts",
    "start:proxy": "cloud_sql_proxy -instances=meteor-247517:us-central1:mysql-instance-dev-b55a41ba=tcp:3306",
    "start:redis": "redis-server"
  },
  "author": "Joseph Shetaye",
  "license": "ISC",
  "_moduleAliases": {
    "@services": "dist/services",
    "@": "dist"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^1.6.0",
    "@google-cloud/storage": "^4.3.1",
    "@types/busboy": "^0.2.3",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.3",
    "@types/moxios": "^0.4.9",
    "@types/node": "^13.7.0",
    "@types/redis": "^2.8.22",
    "@types/sharp": "^0.24.0",
    "@types/supertest": "^2.0.8",
    "algoliasearch": "^4.0.3",
    "axios": "^0.19.2",
    "busboy": "^0.3.1",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "fast-crc32c": "^2.0.0",
    "jest": "^25.1.0",
    "jest-extended": "^0.11.5",
    "jwks-rsa": "^1.7.0",
    "knex": "^0.21.2",
    "module-alias": "^2.2.2",
    "moxios": "^0.4.0",
    "mysql2": "^2.1.0",
    "node-dev": "^4.3.0",
    "prettier": "^1.19.1",
    "redis": "^3.0.2",
    "sharp": "^0.25.1",
    "stream-mock": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node-dev": "^1.0.0-pre.52",
    "tslint": "^6.0.0",
    "typedoc": "^0.16.10",
    "typescript": "^3.7.5"
  }
}
